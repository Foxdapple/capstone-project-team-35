{
  "name": "capstone-project-team-35",
  "productName": "Ocellai",
  "author": "Software Alchemists",
  "description": "This is an insect identificaton app.",
  "version": "1.0.0",
  "homepage": ".",
  "private": true,
  "main": "electron-main.js",
  "proxy": "http://127.0.0.1:5000",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@syncfusion/ej2-react-dropdowns": "^22.2.12",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "d3": "^7.8.5",
    "express": "^4.18.2",
    "flask": "^0.2.10",
    "framer-motion": "^10.16.4",
    "plotly.js": "^2.26.1",
    "plotly.js-dist-min": "^2.26.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropdown": "^1.11.0",
    "react-icons": "^4.10.1",
    "react-plotly.js": "^2.6.0",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-backend": "cd flask-backend && flask run",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "package": "electron-builder build",
    "build-exe": "cd flask-backend/build_script && python build.py"
  },
  "build": {
    "appId": "com.SoftwareAlchemists.Ocellai",
    "extends": null,
    "productName": "Ocellai",
    "files": [
      "build/",
      "package.json",
      "electron-main.js",
      "license.txt",
      "README.md",
      "README_assets/",
      "flask-backend/build_script/packaged_backend/ocellai_backend/",
      "flask-backend/build_script/packaged_backend/ocellai_backend/ocellai_backend.exe",
      "flask-backend/"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "uninstallDisplayName": "Ocellai",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "daisyui": "^3.7.3",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "tailwindcss": "^3.3.3"
  }
}
